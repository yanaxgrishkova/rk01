template <typename Iterator, typename T>
auto find_two_elements_with_sum(Iterator first, Iterator last, T c) -> std::pair<bool, std::pair<Iterator, Iterator>>
{

	std::pair<bool, std::pair<Iterator, Iterator>> sumpair;

	while (*first < *last)
	{

		T sum = *first + *last;
		if (sum == C)
		{
			sumpair.first = true;
			sumpair.second.first = first;
			sumpair.second.second = last;
		}
		else
		{
			if (sum < c) 
        first++;
			else 
        last--;
		}
	}
	return sumpair;
}
